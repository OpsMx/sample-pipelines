    {
      "account": "default",
      "cloudProvider": "kubernetes",
      "manifestArtifactId": "cfdb2b9d-c2c5-44b7-89a6-575d4b545ed6",
      "moniker": {
        "app": "sampleapp"
      },
      "name": "Deploy (Manifest)",
      "namespaceOverride": "default",
      "refId": "2",
      "requisiteStageRefIds": [
        "1"
      ],
      "skipExpressionEvaluation": false,
      "source": "artifact",
      "trafficManagement": {
        "enabled": false,
        "options": {
          "enableTraffic": false,
          "services": []
        }
      },
      "type": "deployManifest"
