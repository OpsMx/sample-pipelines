{
  "application": "sampleapp",
  "description": "This pipeline shows the demo webhook trigger and values in evaluate that are taken from trigger",
  "keepWaitingPipelines": false,
  "lastModifiedBy": "Admin",
  "limitConcurrent": true,
  "parameterConfig": [
    {
      "default": "",
      "description": "",
      "hasOptions": false,
      "label": "email",
      "name": "email",
      "options": [
        {
          "value": ""
        }
      ],
      "pinned": true,
      "required": false
    }
  ],
  "spelEvaluator": "v4",
  "stages": [
    {
      "failOnFailedExpressions": true,
      "name": "Evaluate Variables",
      "refId": "2",
      "requisiteStageRefIds": [],
      "type": "evaluateVariables",
      "variables": [
        {
          "key": "parameter_email",
          "value": "${trigger.parameters.email}"
        },
        {
          "key": "user",
          "value": "${trigger.payload.user}"
        }
      ]
    }
  ],
  "triggers": [
    {
      "enabled": true,
      "payloadConstraints": {
        "user": "myvalue"
      },
      "source": "webhook-trigger",
      "type": "webhook"
    }
  ]
}
