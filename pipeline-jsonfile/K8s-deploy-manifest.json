{
  "application": "sample-app",
  "keepWaitingPipelines": false,
  "lastModifiedBy": "admin",
  "limitConcurrent": true,
  "name": "k8s-deploy-manifest",
  "spelEvaluator": "v4",
  "stages": [
    {
      "account": "default",
      "cloudProvider": "kubernetes",
      "isNew": true,
      "manifests": [
        {
          "apiVersion": "apps/v1",
          "kind": "Deployment",
          "metadata": {
            "name": "nginx-deployment"
          },
          "spec": {
            "replicas": 1,
            "selector": {
              "matchLabels": {
                "app": "nginx"
              }
            },
            "template": {
              "metadata": {
                "labels": {
                  "app": "nginx"
                }
              },
              "spec": {
                "containers": [
                  {
                    "image": "nginx:1.14.2",
                    "name": "nginx",
                    "ports": [
                      {
                        "containerPort": 80
                      }
                    ]
                  }
                ]
              }
            }
          }
        }
      ],
      "moniker": {
        "app": "sample-app"
      },
      "name": "Deploy (Manifest)",
      "refId": "1",
      "requisiteStageRefIds": [],
      "skipExpressionEvaluation": false,
      "source": "text",
      "trafficManagement": {
        "enabled": false,
        "options": {
          "enableTraffic": false,
          "namespace": null,
          "services": [],
          "strategy": null
        }
      },
      "type": "deployManifest"
    },
    {
      "account": "default",
      "app": "sample-app",
      "cloudProvider": "kubernetes",
      "cluster": null,
      "criteria": null,
      "isNew": true,
      "kind": null,
      "kinds": [
        "deployment"
      ],
      "labelSelectors": {
        "selectors": [
          {
            "key": "app",
            "kind": "EQUALS",
            "values": [
              "nginx"
            ]
          }
        ]
      },
      "location": "default",
      "manifestName": null,
      "mode": "label",
      "name": "Delete (Manifest)",
      "options": {
        "cascading": true,
        "gracePeriodSeconds": null
      },
      "refId": "2",
      "requisiteStageRefIds": [
        "1"
      ],
      "type": "deleteManifest"
    }
  ],
  "triggers": []
}
